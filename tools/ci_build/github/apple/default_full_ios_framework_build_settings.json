{
    "build_osx_archs": {
        "iphoneos": [
            "arm64"
        ],
        "iphonesimulator": [
            "arm64",
            "x86_64"
        ],
        "macabi": [
            "arm64",
            "x86_64"
        ]
    },
    "build_params": {
      "base": [
        "--parallel",
        "--build_apple_framework",
        "--skip_tests"
      ],
        "iphoneos": [
            "--ios",
            "--cmake_generator",
            "Xcode",
            "--apple_deploy_target=13.0"
        ],
        "iphonesimulator": [
            "--ios",
            "--cmake_generator",
            "Xcode",
            "--apple_deploy_target=13.0"
        ],
        "macabi":[
            "--macos=Catalyst",
            "--apple_deploy_target=14.0"
        ]
    }
}

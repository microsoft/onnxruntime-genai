{
  "model" : {
    "type": "llama",
    "decoder": "model.onnx",
    "hidden_size": 128,
    "num_attention_heads": 32,
    "num_hidden_layers": 32,
    "vocab_size": 32000,
    "logits_type" : "float32",
    "kv_type" : "float32",
    "past_names_key" : "past_key_values.%d.key",
    "past_names_value" : "past_key_values.%d.value",
    "present_names_key" : "present.%d.key",
    "present_names_value" : "present.%d.value"
  },

  "bos_token_id": 1,
  "eos_token_id": 2,
  "max_length": 4096,
  "min_length": 0,
  "top_p": 0.7,
  "temperature": 0.6
}
